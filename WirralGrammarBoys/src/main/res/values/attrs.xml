<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FloatLabelLayout">
        <attr name="floatLabelTextAppearance" format="reference" />
        <attr name="floatLabelSidePadding" format="reference|dimension" />
    </declare-styleable>
    <declare-styleable name="FloatingActionButton">
        <attr name="drawable" format="integer" />
        <attr name="fabColor" format="reference|color" />
        <attr name="pressedColor" format="reference|color" />
        <attr name="fabRadius" format="dimension" />
        <attr name="shadowRadius" format="dimension" />
        <attr name="shadowDx" format="dimension" />
        <attr name="shadowDy" format="dimension" />
        <attr name="shadowColor" format="integer" />
    </declare-styleable>
</resources>